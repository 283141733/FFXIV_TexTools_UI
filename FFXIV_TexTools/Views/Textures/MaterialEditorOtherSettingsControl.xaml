<UserControl x:Class="FFXIV_TexTools.Views.Textures.MaterialEditorOtherSettingsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FFXIV_TexTools.Views.Textures"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="800">

    <Border BorderBrush="Black" BorderThickness="1" Margin="5">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <TextBlock  Text="Other Settings" TextAlignment="Right" FontWeight="Bold" Grid.Row="0"  FontSize="12" FontStyle="Italic" Margin="0" VerticalAlignment="Center" HorizontalAlignment="Center" />

        </Grid>
    </Border>
</UserControl>
